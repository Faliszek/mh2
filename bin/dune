(executable (name Server) (modules (:standard \ Mh2BinPesyModules))
    (public_name Server.exe) (libraries mh2.bin.pesy-modules)
    (flags -open Mh2BinPesyModules))
(library (public_name mh2.bin.pesy-modules) (name Mh2BinPesyModules)
    (modules Mh2BinPesyModules) (libraries mh2.lib))
(rule
    (with-stdout-to Mh2BinPesyModules.re
        (run echo "module Library = Mh2Lib;")))
