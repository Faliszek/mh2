{
  "name": "mh2",
  "version": "0.0.0",
  "description": "My Project",
  "esy": {
    "build": "refmterr dune build -p #{self.name}",
    "NOTE": "Optional release Section. Customizes result of `esy release`",
    "release": {
      "releasedBinaries": [
        "Server.exe"
      ]
    },
    "buildEnv": {
      "ODOC_SYNTAX": "re"
    }
  },
  "buildDirs": {
    "lib": {
      "imports": [
        "Tablecloth = require('tablecloth-native')",
        "Lwt = require('lwt')",
        "Pgocaml = require('pgocaml')",
        "Jwto = require('jwto')",
        "Graphql_cohttp = require('graphql-cohttp')",
        "Graphql_lwt = require('graphql-lwt')",
        "Cohttp = require('cohttp')",
        "Cohttp_lwt_unix = require('cohttp-lwt-unix')",
        "Safepass = require('safepass')"
      ],
      "preprocess": [
        "pps",
        "ppx_pgsql",
        "ppxlib.runner",
        "lwt_ppx"
      ]
    },
    "bin": {
      "imports": [
        "Library = require('mh2/lib')"
      ],
      "bin": {
        "Server.exe": "Server.re"
      }
    }
  },
  "scripts": {
    "pesy": "bash -c 'env PESY_MODE=update pesy'",
    "test": "esy x TestMh2.exe",
    "start": "esy x Server.exe"
  },
  "dependencies": {
    "@esy-ocaml/reason": "*",
    "@opam/pgocaml": "2.3",
    "@opam/ppx_pgsql": "tizoc/ppx_pgsql:ppx_pgsql.opam",
    "@opam/cohttp": "2.1.2",
    "@opam/cohttp-lwt-unix": "2.0.0",
    "@opam/graphql-cohttp": "0.12.1",
    "@opam/graphql-lwt": "0.9.0",
    "@opam/lwt": "4.1.0",
    "@opam/lwt_ppx": "*",
    "@opam/safepass": "3.0",
    "@opam/yojson": "*",
    "@opam/jwto": "*",
    "@opam/tablecloth-native": "0.0.3",
    "ocaml": "~4.7.0",
    "refmterr": "*"
  },
  "devDependencies": {
    "@opam/merlin": "*",
    "ocaml": "~4.7.0",
    "@opam/odoc": "*",
    "@pesy/esy-pesy": "0.1.0-alpha.2"
  }
}
